fun factorial(n) = 
    if n == 0 then 
        1 
    else 
        n * factorial(n - 1) 
    end

var result = factorial(5)

fun fibonacci(n) = 
    if n <= 1 then 
        n 
    else 
        fibonacci(n - 1) + fibonacci(n - 2) 
    end

